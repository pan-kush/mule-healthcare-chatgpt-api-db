#%RAML 1.0 DataType
type: object
description: status code and response of a transaction.
properties:
  status:
    description: status code of the response.
    type: string
    required: false
  message:
    description: description of the response.
    type: string
    required: false
  transactionId:
    description: description of the response.
    type: string
    required: false